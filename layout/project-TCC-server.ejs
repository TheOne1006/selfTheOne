            </div>
            <div class="col-md-6">
              <img src="<%= require('./assets/svgs/logo/chai.svg') %>" class="svgimg animated long delay-1s">
            </div>
            <div class="col-md-6">
              <img src="<%= require('./assets/svgs/logo/redis.svg') %>" class="svgimg animated long delay-1s">
            </div>
          </div>
        </div>
      </div>
    </div>
